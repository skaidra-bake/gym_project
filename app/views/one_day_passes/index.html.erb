<p id="notice"><%= notice %></p>

<h1>One Day Passes</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Member</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @one_day_passes.each do |one_day_pass| %>
      <tr>
        <td><%= one_day_pass.date %></td>
        <td><%= one_day_pass.member_id %></td>
        <td><%= link_to 'Show', one_day_pass %></td>
        <td><%= link_to 'Edit', edit_one_day_pass_path(one_day_pass) %></td>
        <td><%= link_to 'Destroy', one_day_pass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New One Day Pass', new_one_day_pass_path %>
